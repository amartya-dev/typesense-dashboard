# Typesense Dashboard Environment Configuration
# Copy this file to .env and update the values as needed

# Application Configuration
NODE_ENV=production
REACT_APP_NAME=Typesense Dashboard

# Typesense Server Configuration
REACT_APP_TYPESENSE_SERVER_URL=http://localhost:8108
REACT_APP_TYPESENSE_API_KEY=your-admin-api-key

# Optional: Analytics Configuration
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ANALYTICS_INDEX_NAME=analytics_queries

# Optional: Security Configuration
REACT_APP_ENABLE_HTTPS=false
REACT_APP_CORS_ORIGIN=*

# Optional: Performance Configuration
REACT_APP_CACHE_TTL=3600
REACT_APP_REQUEST_TIMEOUT=30000 